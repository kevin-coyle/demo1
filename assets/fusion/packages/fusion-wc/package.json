{
  "name": "fusion-wc",
  "version": "0.6.0",
  "description": "[DS Web Components] is a library of presentational UI web components to be consumed by [COMPANY NAME] web applications.",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "yarn build:theme-1 && yarn build:theme-2",
    "build:theme-1": "del-cli dist/theme-1 && webpack && tsc",
    "build:theme-2": "del-cli dist/theme-2 && webpack --env theme=theme-2 && tsc --outDir dist/theme-2",
    "build:watch": "webpack --watch",
    "build:tokens": "node token-config.js",
    "build:tokens:core": "npm run build:tokens -- --theme=core",
    "build:tokens:themes": "npm run build:tokens:theme-1",
    "build:tokens:theme-1": "npm run build:tokens -- --theme=theme-1",
    "build:custom-elements.json": "wca analyze \"src/components/*/*.ts\" --outFile custom-elements.json",
    "build:icons": "del-cli ./icons/fusion-icons.svg && svg-sprite-generate -d ./icons/ -o ./icons/fusion-icons.svg",
    "test": "jest",
    "serve": "start-storybook -p 6006",
    "sass:text-passage": "sass --watch components/text-passage/text-passage.scss components/text-passage/text-passage.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "plop": "plop component",
    "plop:page": "plop page",
    "plop:recipe": "plop recipe",
    "lint": "eslint ./src/ --ext .ts",
    "chromatic": "npx chromatic --project-token=df1dfe2f11df"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "",
  "license": "BSD-3-Clause",
  "files": [
    "dist"
  ],
  "exports": {
    "./dist/theme-1/theme-1.css": "./dist/theme-1/theme-1.css",
    "./dist/theme-1/icons/*": "./dist/theme-1/icons/*.svg",
    "./dist/theme-1/data/tokens": "./dist/theme-1/data/tokens",
    "./design-tokens/theme-1/head.scss": "./design-tokens/theme-1/head.scss",
    "./dist/theme-1/components/*": "./dist/theme-1/components/*",
    "./dist/theme-2/theme-2.css": "./dist/theme-2/theme-2.css",
    "./dist/theme-2/icons/*": "./dist/theme-2/icons/*.svg",
    "./dist/theme-2/data/tokens": "./dist/theme-2/data/tokens",
    "./design-tokens/theme-2/head.scss": "./design-tokens/theme-2/head.scss",
    "./dist/theme-2/components/*": "./dist/theme-2/components/*",
    "./components/*/*": "./dist/theme-1/components/*/*"
  },
  "dependencies": {
    "@storybook/addon-storyshots": "^6.4.13",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^11.0.0",
    "lit": "^2.0.0-rc.1",
    "moment": "^2.29.1",
    "nanoid": "^3.1.25"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@etchteam/storybook-addon-status": "^4.2.1",
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/web-components": "^6.4.13",
    "@types/resize-observer-browser": "^0.1.7",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.10.2",
    "clone": "^1.0.4",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-decorator-position": "^2.2.28",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-storybook": "^0.5.5",
    "file-loader": "^6.2.0",
    "glob": "^7.1.7",
    "husky": "^7.0.1",
    "jest": "^27.2.4",
    "js-yaml": "^3.14.1",
    "lint-staged": "^11.0.1",
    "lit-html": "^2.0.0-rc.2",
    "loader-utils": "^2.0.0",
    "markdown-loader": "^8.0.0",
    "mini-css-extract-plugin": "^2.2.1",
    "optimist": "^0.6.1",
    "plop": "^2.7.4",
    "raw-loader": "^4.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^10.2.0",
    "style-dictionary": "^3.1.1",
    "svg-sprite-generator": "0.0.7",
    "typescript": "^3.8.3",
    "web-component-analyzer": "^1.1.6",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2"
  },
  "resolutions": {
    "react": "17.0.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
